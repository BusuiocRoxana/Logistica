<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="disertatie.com.disertatie">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.DateInterneActivity" />
        <activity android:name=".activities.MaterialsActivity" />
        <activity android:name=".activities.FurnizoriActivity" />
        <activity android:name=".activities.AddMaterialActivity" />
        <activity android:name=".activities.AddFurnizorActivity" />
        <activity android:name=".activities.CerereOfertaActivity" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".activities.ListaCereriOfertaActivity" />
        <activity android:name=".activities.ComandaActivity" />
        <activity android:name=".activities.AdaugaTaxeActivity" />
        <activity android:name=".activities.ListaComenziActivity" />
        <activity android:name=".activities.ReceptieActivity" />
        <activity android:name=".activities.TaxeActivity"></activity>
    </application>

</manifest>